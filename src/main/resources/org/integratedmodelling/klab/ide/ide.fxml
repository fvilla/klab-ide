<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.*?>

<BorderPane prefHeight="960.0" prefWidth="1280.0" style="-fx-background-color: -color-neutral-subtle;"
            xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.integratedmodelling.klab.ide.KlabIDEController">
    <left>
        <BorderPane prefHeight="960.0" prefWidth="69.0" BorderPane.alignment="CENTER">
            <center>
                <VBox alignment="TOP_CENTER" prefWidth="52.0" spacing="8.0">
                    <children>
                        <ImageView fitHeight="48.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@icons/klab-elephant.png"/>
                            </image>
                            <VBox.margin>
                                <Insets bottom="8.0" top="12.0"/>
                            </VBox.margin>
                        </ImageView>
                        <Separator prefWidth="200.0"/>
                        <FontIcon iconLiteral="mdal-home" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="homeButton"/>
                        <FontIcon iconLiteral="bi-border-all" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="workspacesButton"/>
                        <FontIcon iconLiteral="wi-earthquake" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="digitalTwinsButton"/>
                        <FontIcon iconLiteral="mdal-help_outline" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="helpButton"/>
                        <Separator prefWidth="200.0"/>
                        <FontIcon iconLiteral="mdal-chevron_right" iconSize="24" strokeType="OUTSIDE"
                                  strokeWidth="0.0" fx:id="rightButton"/>
                        <FontIcon iconLiteral="mdal-chevron_left" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="leftButton"/>
                    </children>
                </VBox>
            </center>
            <bottom>
                <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0"
                      BorderPane.alignment="CENTER">
                    <children>
                        <FontIcon iconLiteral="mdal-get_app" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="downloadButton"/>
                        <FontIcon iconLiteral="mdmz-power_settings_new" iconSize="24" strokeType="OUTSIDE"
                                  strokeWidth="0.0" fx:id="startButton"/>
                        <FontIcon iconLiteral="bi-hdd-stack" iconSize="24" layoutX="32.0" layoutY="124.0"
                                  strokeType="OUTSIDE" strokeWidth="0.0" fx:id="reasonerButton"/>
                        <FontIcon iconLiteral="bi-hdd-stack" iconSize="24" layoutX="32.0" layoutY="199.0"
                                  strokeType="OUTSIDE" strokeWidth="0.0" fx:id="resourcesButton"/>
                        <FontIcon iconLiteral="bi-hdd-stack" iconSize="24" layoutX="32.0" layoutY="199.0"
                                  strokeType="OUTSIDE" strokeWidth="0.0" fx:id="resolverButton"/>
                        <FontIcon iconLiteral="bi-hdd-stack" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="runtimeButton"/>
                        <Separator prefWidth="200.0"/>
                        <FontIcon iconLiteral="fas-cog" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="settingsButton"/>
                        <FontIcon iconLiteral="mdal-lightbulb" iconSize="24" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="inspectorButton"/>
                        <FontIcon iconLiteral="mdmz-person" iconSize="36" strokeType="OUTSIDE" strokeWidth="0.0"
                                  fx:id="profileButton"/>
                    </children>
                    <padding>
                        <Insets bottom="8.0"/>
                    </padding>
                </VBox>
            </bottom>
        </BorderPane>
    </left>
    <center>
        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <left>
                <Pane id="browsingArea" prefHeight="200.0" prefWidth="200.0" visible="false"
                      BorderPane.alignment="CENTER"/>
            </left>
            <center>
                <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <center>
                        <Pane id="mainArea" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </center>
                    <bottom>
                        <Pane id="inspectorArea" prefHeight="304.0" prefWidth="1010.0" BorderPane.alignment="CENTER"/>
                    </bottom>
                </BorderPane>
            </center>
        </BorderPane>
    </center>
</BorderPane>
